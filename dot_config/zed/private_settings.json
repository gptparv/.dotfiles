{
  "auto_install_extensions": {
    "basher": true,
    "catppuccin": true,
    "catppuccin-icons": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "fish": true,
    "ini": true,
    "pylsp": true,
    "ruff": true,
    "terraform": true,
    "toml": true
  },
  "autosave": "on_focus_change",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 16,
  "confirm_quit": true,
  "cursor_shape": "underline",
  "file_icons": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "blinking": "on",
    "font_family": "JetBrainsMono Nerd Font"
  },
  "theme": {
    "dark": "Catppuccin Macchiato (Blur)",
    "light": "Catppuccin Macchiato (Blur)",
    "mode": "system"
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 16
}
